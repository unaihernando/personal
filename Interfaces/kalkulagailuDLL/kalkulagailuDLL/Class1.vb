
Public Class Class1
    Public Function gehitu(zbk As Double, zbk2 As Double) As Double
        Return zbk + zbk2
    End Function
    Public Function kendu(zbk As Double, zbk2 As Double) As Double
        Return zbk - zbk2
    End Function

    Public Function bider(zbk As Double, zbk2 As Double) As Double
        Return zbk * zbk2
    End Function

    Public Function zati(zbk As Double, zbk2 As Double) As Double
        Return zbk / zbk2
    End Function

End Class
